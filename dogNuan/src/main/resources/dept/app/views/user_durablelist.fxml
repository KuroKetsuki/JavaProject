<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="durableListBackground" prefHeight="600.0" prefWidth="800.0" stylesheets="@../Theme/pastel.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dept.app.controllers.user.DurablesListController">
   <children>
      <Button fx:id="backButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#onBackButtonClick" text="Back" />
      <Label fx:id="durableListLabel" layoutX="79.0" layoutY="77.0" text="Durable Articles List">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TableView fx:id="durableTableView" layoutX="79.0" layoutY="113.0" prefHeight="434.0" prefWidth="641.0">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
      </TableView>
      <TextField fx:id="textSearch" layoutX="518.0" layoutY="78.0" />
      <Button fx:id="searchButton" layoutX="668.0" layoutY="78.0" mnemonicParsing="false" onAction="#searchButtonClick" text="Search" />
   </children>
</AnchorPane>
